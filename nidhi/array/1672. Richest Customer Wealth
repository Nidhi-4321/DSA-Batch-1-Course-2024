class Solution {
    public int maximumWealth(int[][] accounts) {

        int sum1 = 0;
        for (int i=0; i<accounts.length;i++) {
            int sum2 = 0;
            for (int j=0; j<accounts.length;j++) {
                sum2 = account[i][j];
            }
            sum1 = Math.max(sum1,sum2);
        }
        return sum1;
        
    }
}
